{
  "name": "strapieno/str-user-api",
  "homepage": "https://github.com/strapieno/str-user-api",
  "require": {
    "php": ">=5.5.0",
    "zendframework/zendframework": "~2.5",
    "zfcampus/zf-apigility-provider": "~1.0",
    "zfcampus/zf-content-negotiation": "~1.0",
    "zfcampus/zf-content-validation": "~1.0",
    "zfcampus/zf-rpc": "~1.0",
    "zfcampus/zf-rest": "~1.0",
    "zfcampus/zf-versioning": "~1.0",
    "zfcampus/zf-apigility-documentation": "~1.0",
    "zfcampus/zf-configuration": "~1.0",
    "zfcampus/zf-console": "~1.0",
    "strapieno/str-user-model" : "dev-develop",
    "strapieno/str-model-utils": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "zendframework/zftool": "dev-master",
    "robertboloc/zf2-components-list-generator": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Strapieno\\User\\Model\\": "src/",
      "Strapieno\\User\\ModelTest\\": "tests/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:strapieno/str-user-model.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:strapieno/str-model-utils.git"
    }
  ],
  "authors": [
    {
      "name": "Antonio Visalli",
      "email": "antonino.visalli@gmail.com",
      "homepage": "http://github.com/visa4"
    },
    {
      "name": "Leonardo Di Donato",
      "email": "leodidonato@gmail.com",
      "homepage": "http://github.com/leodido"
    },
    {
      "name": "Leonardo Grasso",
      "email": "me@leonardograsso.com",
      "homepage": "http://github.com/leogr"
    }
  ]
}
